{
  "name": "make-it-rain",
  "version": "1.4.0",
  "description": "Pull your Raindrop.io bookmarks into Obsidian notes with flexible filtering and location options",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json && npm run build"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "raindrop"
  ],
  "author": "frostmute",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.17.3",
    "obsidian": "^1.8.7",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
